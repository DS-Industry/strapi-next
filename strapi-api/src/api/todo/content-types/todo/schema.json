{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "CreatedAt": {
      "type": "datetime"
    },
    "UpdatedAt": {
      "type": "datetime"
    },
    "body": {
      "type": "text"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "status_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::status.status",
      "inversedBy": "todos"
    },
    "createdUserBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "todos"
    },
    "executed_bies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "todos_executed"
    },
    "ticket_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ticket.ticket",
      "inversedBy": "todos"
    },
    "department_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "todos"
    },
    "category_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "todos"
    },
    "subcategory_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "todos"
    },
    "priority_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::priority.priority",
      "inversedBy": "todos"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "todo_id"
    }
  }
}
