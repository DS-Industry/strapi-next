{
  "kind": "collectionType",
  "collectionName": "statuses",
  "info": {
    "singularName": "status",
    "pluralName": "statuses",
    "displayName": "Status"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "todos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todo.todo",
      "mappedBy": "status_id"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "status"
    }
  }
}
