{
  "kind": "collectionType",
  "collectionName": "ticket_sources",
  "info": {
    "singularName": "ticket-source",
    "pluralName": "ticket-sources",
    "displayName": "ticketSource"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "ticketSource"
    }
  }
}
