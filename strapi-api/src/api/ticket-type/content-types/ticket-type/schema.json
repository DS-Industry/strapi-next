{
  "kind": "collectionType",
  "collectionName": "ticket_types",
  "info": {
    "singularName": "ticket-type",
    "pluralName": "ticket-types",
    "displayName": "ticketType",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "ticketTasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket-task.ticket-task",
      "mappedBy": "ticketType"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "ticketTypes"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "ticketType"
    }
  }
}
